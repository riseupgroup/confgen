{{#if note}}# Note: {{note}}{{/if}}
[Interface]
PrivateKey = {{privkey}}
ListenPort = {{listen_port}}
Table = off
{{#unless mp_bgp}}PostUp = ip -4 addr add dev %i {{self_ll_ipv4}}/32 peer {{peer_ll_ipv4}}/32{{/unless}}
PostUp = ip -6 addr add dev %i {{self_ll_ipv6}}/128 peer {{peer_ll_ipv6}}/128

[Peer]
PublicKey = {{peer_pubkey}}
Endpoint =  {{peer_endpoint}}
AllowedIPs = 10.0.0.0/8, 172.20.0.0/14, 172.31.0.0/16, fd00::/8, fe80::/64
PersistentKeepalive = 25
